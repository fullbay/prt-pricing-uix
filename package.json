{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8090",
    "build": "tsc && vite build",
    "build:watch": "vite build --watch",
    "build:preview": "npm run build && vite preview --port 8090",
    "preview": "vite preview --port 8090",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier -c .",
    "format:all": "npm run format && npm run lint",
    "format:fix": "prettier --write .",
    "generate": "graphql-codegen --config codegen.ts",
    "prepare": "husky",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest -u",
    "clean": "rm -rf node_modules && npm ci ",
    "tc": "tsc --project tsconfig.app.json --noEmit"
  },
  "dependencies": {
    "@fullbay/forge": "^1.3.10",
    "@stytch/react": "^19.11.1",
    "@stytch/vanilla-js": "^5.35.1",
    "@tanstack/react-query": "^5.76.1",
    "@tanstack/react-table": "^8.21.3",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "graphql-tag": "^2.12.6",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.3",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-i18next": "^15.4.0",
    "react-router-dom": "^7.6.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.2",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-operations": "^5.0.2",
    "@module-federation/vite": "^1.2.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.7.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.5",
    "dotenv": "^17.2.3",
    "eslint": "^9.19.0",
    "eslint-plugin-perfectionist": "^4.9.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  }
}
